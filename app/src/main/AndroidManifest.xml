<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="androidsupersquad.rocketfrenzy">

    <uses-sdk android:minSdkVersion="15" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:screenOrientation="portrait"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Title.Title" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
        </activity>

        <provider android:name="androidsupersquad.rocketfrenzy.DataBase.RocketContentProvider"
            android:authorities="androidsupersquad.rocketfrenzy.DataBase"
            android:exported="true">
        </provider>


        <activity android:name=".MiniGame.ShakeMiniGame" android:screenOrientation="portrait"/>
        <activity android:name=".MiniGame.Lottery" android:screenOrientation="portrait"></activity>
        <activity android:name=".RocketLaunch" android:screenOrientation="portrait"></activity>

    </application>

</manifest>