<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="androidsupersquad.rocketfrenzy.Fragments.KamikaziFragment">


    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="166dp"
        android:layout_height="102dp"
        app:srcCompat="@drawable/christian"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="188dp"
        android:layout_height="121dp"
        app:srcCompat="@drawable/daniel"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="336dp"
        android:layout_height="349dp"
        app:srcCompat="@drawable/trigger"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView11"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="100dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/jimmy"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/eugeneshead"
        android:layout_alignTop="@+id/imageView13"
        android:layout_toStartOf="@+id/imageView12"
        android:layout_marginTop="78dp" />
</RelativeLayout>
